        "type": "git",
        "url": "https://github.com/nablify/web.git"
    },
    "dependencies": {
        "bootstrap": "5.2.3",
        "bootstrap-icons": "^1.11.1"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "3.0.3",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "8.4.21",
        "prettier": "2.8.6",
